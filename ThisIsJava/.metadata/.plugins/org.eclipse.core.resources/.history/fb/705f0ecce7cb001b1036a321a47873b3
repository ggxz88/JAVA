package chap07.section04;

public class SupersonicAirplane {
	public static final int NORMAL = 1;
	public static final int SUPERSONIC = 2;
	
	public int flyMode = NORMAL;
	
	@Override
	public void fly() {
		if(flyMode == SUPERSONIC) {
			System.out.println("초음속 비행입니다.");
		} else {
			//Airplane의 fly()
			super.fly();
		}
	}
}
